	date-modified = {2009-03-03 15:20:12 +0000},
	pages = {AIAA 2004-6438},
	publisher = {American Institute of Aeronautics and Astronautics Press},
	title = {Multiobjective evolutionary design of steel structures in tall buildings},
	year = {2004},
