	id = {Birch2019},
	isbn = {1619-1374},
	journal = {Software \& Systems Modeling},
	number = {1},
	pages = {445--471},
	title = {Fast test suite-driven model-based fault localisation with application to pinpointing defects in student programs},
	ty = {JOUR},
