	pages = {297--310},
	publisher = {ACM Press},
	title = {Value Dependence Graphs: Representation Without Taxation},
	url = {http://www.acm.org/pubs/articles/proceedings/plan/174675/p297-weise/p297-weise.pdf},
	year = {1994},
